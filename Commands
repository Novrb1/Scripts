    sys("Server mute Off",255,255,255)
    smute = false
end)
mouse.KeyDown:Connect(function(key)
    if key == kEy then
       lp.Character.HumanoidRootPart.Anchored = not lp.Character.HumanoidRootPart.Anchored 
       if lp.Character.HumanoidRootPart.Anchored == false then
           sys("UnFroze",255,0,0)
       end
       if lp.Character.HumanoidRootPart.Anchored == true then
           sys("Froze",255,0,0)
       end
    end
end)
game.Players.LocalPlayer.Chatted:Connect(function(Msg)
    CheckCmd(Msg)
end)
Help()
notif("Thanks For Using","Commands In Console",5)
function cadd()
    lp.Character.ChildAdded:Connect(function(thing)
        if ac then
            if thing:IsA'Tool' then
                if thing.Name == "nilthisplease" then
                    
                else
                    wait()
                    thing.Parent = lp.Backpack
                end
            end
        end
    end)
end
cadd()
lp.CharacterAdded:Connect(function()
    cadd()
end)
rservice.Stepped:Connect(function()
    pcall(function()
        --credits synapse discord dev for fps cap
        setfpscap(1e6)
        if noClip == true then
            for _,z in pairs(lp.Character:GetChildren()) do
                if z:IsA'Part' or z:IsA'MeshPart' then
                    z.CanCollide = false 
                end
            end
        end
        if cam == true then
            lp.CameraMinZoomDistance = math.huge - math.huge
            lp.CameraMaxZoomDistance = math.huge - math.huge 
        end
        if cam == false then
            lp.CameraMinZoomDistance = 0.05
            lp.CameraMaxZoomDistance = math.huge
        end
        if smute then
           for _,z in pairs(game.Players:GetPlayers()) do
               if z.Name ~= lp.Name then
                   for _,v in pairs(z.Character:GetDescendants()) do
                       if v:IsA'Sound' then
                          v.Playing = false 
                       end
                   end
               end
           end
        end
    end)
end)
