        if Args[1] and Args[1] ~= "" and Args[1] ~= " " and tonumber(Args[1]) then
            lp.Character.Humanoid.JumpPower = tonumber(Args[1])
            notif("JumpPower Set To" .. " " .. tonumber(Args[1]))
        else
            notif("Need Second Argument, Need Number")
        end
    end
