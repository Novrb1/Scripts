AddCmd(
    "etools",
    "-Equips Tools",
    function(Args)
        for _, z in next, lp.Backpack:GetChildren() do
            if z:IsA "Tool" then
                z.Parent = lp.Character
            end
        end
        notif("Equip Tools")
    end
)
AddCmd(
    "fov",
    "-Field Of View Changer",
    function(Args)
        if Args[1] and Args[1] ~= "" and Args[1] ~= " " and tonumber(Args[1]) <= 120 then
            workspace.Camera.FieldOfView = Args[1]
            notif("FOV Changed To " .. Args[1])
        else
            notif("Need Second Argument, Need Lower Number")
        end
    end
)
AddCmd(
    "noclip",
    "-Noclips Character",
    function(Args)
        noClip = true
        notif("Noclip On")
    end
)
AddCmd(
    "clip",
    "-Unclips Character",
    function(Args)
        noClip = false
        notif("Noclip Off")
    end
)
AddCmd(
    "infcam",
    "-Max's The Cameras View Limit",
    function(Args)
        lp.CameraMinZoomDistance = 0.05
        lp.CameraMaxZoomDistance = math.huge
        notif("Infinite Camera")
    end
)
AddCmd(
    "anims",
    "-Changes Anim Speed",
    function(Args)
        if tonumber(Args[1]) and tonumber(Args[1]) ~= "" and tonumber(Args[1]) ~= " " then
            local Hum =
                lp.Character:FindFirstChild("Humanoid") or lp.Character:FindFirstChildOfClass("AnimationController")
            notif("Animation Speed Changed To " .. tonumber(Args[1]))
            for _, z in next, Hum:GetPlayingAnimationTracks() do
                z:AdjustSpeed(tonumber(Args[1] or 1))
            end
        else
            notif("Need Second Argument, Need Number")
        end
    end
)
AddCmd(
    "smute",
    "-Server Mutes Boombox's",
    function(Args)
        smute = true
        notif("Server mute On")
    end
)
AddCmd(
    "unsmute",
    "-UnServer Mutes Boombox's",
    function(Args)
        smute = false
        notif("Server mute Off")
    end
)
AddCmd(
    "tpa",
    "-Changes TimePos",
    function(Args)
        if tonumber(Args[1]) and tonumber(Args[1]) ~= "" and tonumber(Args[1]) ~= " " then
            for _, z in next, game:GetDescendants() do
                if z:IsA "Sound" then
                    z.TimePosition = Args[1]
                end
            end
            notif("TimePos Changed")
        else
            notif("Need Second Argument, Need Number")
        end
    end
)
AddCmd(
    "freeze",
    "-Freeze's Character",
    function(Args)
        for _, z in next, lp.Character:GetDescendants() do
            if z:IsA "Part" or z:IsA "MeshPart" then
                if z.Name ~= "Handle" then
                    z.Anchored = true
                end
            end
        end
        notif("Froze Character")
    end
)
AddCmd(
    "unfreeze",
    "-UnFreeze's Character",
    function(Args)
        for _, z in next, lp.Character:GetDescendants() do
            if z:IsA "Part" or z:IsA "MeshPart" then
                if z.Name ~= "Handle" then
                    z.Anchored = false
                end
            end
        end
        notif("UnFroze Character")
    end
)
AddCmd(
    "spec",
    "-Spectates Player",
    function(Args)
        if Args[1] and Args[1] ~= "" and Args[1] ~= " " then
            for _, v in next, game.Players:GetPlayers() do
                if v.Name:lower():sub(1, #Args[1]) == Args[1]:lower() then
                    if v and v.Character then
                    else
                        notif("Cannot Spectate", 1)
                    end
                    workspace.CurrentCamera.CameraSubject = v.Character.Head
                    v.Character:FindFirstChildOfClass("Humanoid").Died:Connect(
                        function()
                            workspace.CurrentCamera.CameraSubject = lp.Character.Humanoid
                        end
                    )
                end
            end
            notif("Spectating Player")
        else
            notif("Need Second Argument")
        end
    end
)
AddCmd(
    "unspec",
    "Unspectates / Fixes Camera",
    function(Args)
        workspace.CurrentCamera.CameraSubject = lp.Character:FindFirstChildOfClass("Humanoid")
        workspace.CurrentCamera.CameraType = "Custom"
        lp.CameraMinZoomDistance = 0.05
        lp.CameraMaxZoomDistance = 400
        lp.CameraMode = "Classic"
    end
)
cmdBar.FocusLost:Connect(
    function()
        cmdBar.Position = UDim2.new(0, 99999, 0, 99999)
        CheckCmd(cmdBar.Text)
    end
)
Help()
function cadd()
    lp.Character.ChildAdded:Connect(
        function(thing)
            if ac then
                if thing:IsA "Tool" and thing.Name ~= "nillingprocessD" then
                    thing.Parent = lp.Backpack
                    wait()
                    thing.Parent = lp.Backpack
                end
            end
        end
    )
end
cadd()
lp.CharacterAdded:Connect(
    function()
        cadd()
    end
)
game.Players.PlayerRemoving:Connect(
    function(PlayerRemoving)
        if game.CoreGui:FindFirstChild(PlayerRemoving.Name) then
            game.CoreGui:FindFirstChild(PlayerRemoving.Name):Destroy()
        end
    end
)
rservice.Stepped:Connect(
    function()
        pcall(
            function()
                --credits synapse discord dev for fps cap
                setfpscap(1e6)
                for _, z in next, game.CoreGui:GetChildren() do
                    if z:IsA "BoxHandleAdornment" then
                        for _, v in next, game.Players:GetPlayers() do
                            if z.Name == v.Name then
                                z.Adornee = v.Character.Head
                            end
                        end
                    end
                end
                if noClip == true then
                    for _, z in next, lp.Character:GetChildren() do
                        if z:IsA "Part" or z:IsA "MeshPart" then
                            z.CanCollide = false
                        end
                    end
                end
                if smute then
                    for _, z in next, game.Players:GetPlayers() do
                        if z.Name ~= lp.Name then
                            for _, v in next, z.Character:GetDescendants() do
                                if v:IsA "Sound" then
                                    v.Playing = false
                                end
                            end
                        end
                    end
                end
                if stopit then
                    for _, z in next, workspace:GetChildren() do
                        if z:IsA "Tool" then
                            pcall(
                                function()
                                    z.Handle.CFrame = lp.Character.HumanoidRootPart.CFrame
                                end
                            )
                        end
                    end
                end
            end
        )
    end
)
notif("Feens#7908 :D")
