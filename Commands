AddCmd(
    "info",
    "-Checks Name/Age/Checks Friends Sends To Workspace",
    function(Args)
        if Args[1] and Args[1] ~= "" and Args[1] ~= " " then
            for _, v in next, game.Players:GetPlayers() do
                if v.Name:lower():sub(1, #Args[1]) == Args[1]:lower() then
                    writefile(
                        "Playerss/" .. v.Name .. ".txt",
                        "Name: " ..
                            v.Name ..
                                " | Age: " ..
                                    v.AccountAge ..
                                        " | Roblox Profile: https://www.roblox.com/users/" .. v.UserId .. "/profile"
                    )
                    for _, f in next, game.Players:GetPlayers() do
                        if f:IsFriendsWith(v.UserId) then
                            writefile(
                                "Playerss/" .. v.Name .. " Friend.txt",
                                "Name: " ..
                                    f.Name ..
                                        " | Age: " ..
                                            f.AccountAge ..
                                                " | Roblox Profile: https://www.roblox.com/users/" ..
                                                    f.UserId .. "/profile"
                            )
                        end
                    end
                end
            end
            notif("Info In Workspace/Playerss")
        else
            notif("Need Second Argument")
        end
    end
)
AddCmd(
    "rj",
    "-Rejoins",
    function(Args)
        notif("Rejoining...")
        game:GetService("TeleportService"):Teleport(game.PlaceId, lp)
    end
)
AddCmd(
    "goto",
    "-Tps To Player",
    function(Args)
        if Args[1] and Args[1] ~= "" and Args[1] ~= " " then
            for _, v in next, game.Players:GetPlayers() do
                if v.Name:lower():sub(1, #Args[1]) == Args[1]:lower() then
                    if v and lp and v.Character and lp.Character and lp.Character:FindFirstChild("HumanoidRootPart") and v.Character:FindFirstChild("Head") then
                        for _,z in pairs(lp.Character:GetChildren()) do
                            if z:IsA'BasePart' then
                                z.Velocity = v3n(0,0,0)
                            end
                        end
                        lp.Character.HumanoidRootPart.CFrame = v.Character.Head.CFrame
                    else
                        notif("Cannot TP To Person No Character")
                    end
                end
            end
        else
            notif("Need Second Argument")
        end
    end
)
AddCmd(
    "esp",
    "-ESP's Player",
    function(Args)
        if Args[1] and Args[1] ~= "" and Args[1] ~= " " then
            for _, v in next, game.Players:GetPlayers() do
                if v.Name:lower():sub(1, #Args[1]) == Args[1]:lower() then
                    if v and v.Character and v.Character:FindFirstChild("Head") then
                        selp(v)
                        notif("Esp'd" .. " " .. v.Name)
                    else
                        notif("Cant Esp" .. " " .. v.Name)
                    end
                end
            end
        else
            notif("Need Second Argument")
        end
    end
)
AddCmd(
    "unesp",
    "-UnESP's Player",
    function(Args)
        if Args[1] and Args[1] ~= "" and Args[1] ~= " " then
            for _, v in next, game.Players:GetPlayers() do
                if v.Name:lower():sub(1, #Args[1]) == Args[1]:lower() then
                    if game.CoreGui:FindFirstChild(v.Name) then
                        game.CoreGui:FindFirstChild(v.Name):Destroy()
                        notif("UnEsp'd" .. " " .. v.Name)
                    else
                        notif("Cant UnEsp" .. " " .. v.Name)
                    end
                end
            end
        else
            notif("Need Second Argument")
        end
    end
)
AddCmd(
    "cl",
    "-Claims Player [[ToolsNeeded]]",
    function(Args)
        if Args[1] and Args[1] ~= "" and Args[1] ~= " " then
            for _, v in next, game.Players:GetPlayers() do
                if v.Name:lower():sub(1, #Args[1]) == Args[1]:lower() then
                    if lp and lp.Character and v and v.Character then
                    else
                        notif("Cannot Kill")
                    end
                    clone(true)
                    local num = 0
                    repeat
                        wait()
                        pcall(
                            function()
                                lp.Character.HumanoidRootPart.CFrame = v.Character.HumanoidRootPart.CFrame
                                num = num + 1
                            end
                        )
                    until num == 15
                    lp.Character.HumanoidRootPart.CFrame = cfn(0, workspace.FallenPartsDestroyHeight + 5, 0)
                end
            end
            notif("Killed Player")
        else
            notif("Need Second Argument")
        end
    end
)
